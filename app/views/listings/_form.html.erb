<%= form_for @listing do |form| %>

	<div>
	  <%= form.label :avatar %>
	  <%= form.file_field :avatar, multiple: true %>
	</div>
	
	<div>
		<%= form.label :category %>
		<%= form.text_field :category %> 
	</div>

	<div>
	  <%= form.label :price %>
	  <%= form.text_field :price %> 
	</div>

	<div class="submit-field">
	  <%= form.submit %>
	</div>
	
<% end %>